<% if(typeof errors !== 'undefined') { %>
    <% errors.forEach(function(error) { %>
        <div class="alert alert-danger">
            <%= error.msg %>
        </div>
    <% }) %>
<% } %>

<% if (success_msg != '') { %>
    <div class="alert alert-success"><%= success_msg %> <br> </div>
<% } %>

<% if (error_msg != '') { %>
    <div class="alert alert-danger"><%= error_msg %> <br></div>
<% } %>

<% if (error != '') { %>
    <div class="alert alert-danger"><%= error %> <br></div>
<% } %>